{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "React Playground",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:bundleanalyzer": "ANALYZE_WEBPACK_BUNDLE yarn build",
    "dev": "webpack-dev-server",
    "dev:bundleanalyzer": "ANALYZE_WEBPACK_BUNDLE=true yarn dev",
    "nuke": "rm -rf dist node_modules",
    "prebuild": "rm -rf dist",
    "start": "node ./server.js"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "react-hot-loader": "^4.12.20",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "keywords": [
    "react",
    "react 16",
    "react router 5",
    "webpack 4",
    "hot module replacement",
    "hmr"
  ]
}
